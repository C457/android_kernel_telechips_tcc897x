	};
*/

	//M95 MICOM PD10
	m95micompd10 {
		compatible = "telechips, m95micompd10";
		m95micompd10-gpio = <&gpg 5 0>; 
	};
	/* DAUDIO END */
	
	/* CP */
	auth-cp {
		compatible = "telechips, tcc-cp";
		/* DAUDIO BEGIN */
		/*
		cp-type = <1>;      // 2.0C = 1, 2.0B = 0
		i2c-channel = <2>;      // 0 ~ 3
		 */
