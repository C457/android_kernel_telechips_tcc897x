#include "tcc/tcc8971.dtsi"

/ {
	model = "Telechips TCC8971 LCN Board";
	compatible = "telechips,tcc8971-lcn", "telechips,tcc897x";

	chosen {
		/* DAUDIO BEGIN */
		/*
		bootargs = "vmalloc=480M console=ttyS0,115200n8";
		*/
		bootargs = "vmalloc=480M console=ttyTCC0,115200n8";
		/* DAUDIO END */
		linux,initrd-start = <0x81000000>;
		linux,initrd-end = <0x81400000>;
	};

	ckc@74000000 {
		adjust-audio-clk;	/* adjust audio clock for A2DP plaing */
	};

	/* console */
	serial@76370000 {
		/*pinctrl-names = "default";*/
		pinctrl-names = "default", "idle", "active";
		pinctrl-0 = <&uart22_data>;
		pinctrl-1 = <&uart22_idle>;
		pinctrl-2 = <&uart22_data>;
		debug-gpio = <&gpc 22 0>;
		status = "okay";
	};

	/* bluetooth */
	serial@76380000 {
		pinctrl-names = "default";
		/* DAUDIO BEGIN*/
		/*
		pinctrl-0 = <&uart29_data &uart29_rtscts>;
		*/
		pinctrl-0 = <&uart26_data &uart26_rtscts>;
		/* DAUDIO END */
		status = "okay";
	};

	bluetooth {
		compatible = "telechips, tcc-bluetooth";
		/* DAUDIO BEGIN*/
		/*
		bt_reg_on-gpio = <&gpsd 10 0>;
		wifi_reg_on-gpio = <&gpf 30 0>;
		*/
		bt_reset-gpio = <&gpb 20 0>;
		wifi_reg_on-gpio = <&gpb 14 0>;
		/* DAUDIO END */
		status = "okay";
	};
	
	/* DAUDIO BEGIN */	
	tcc_bt_dev {
		compatible = "telechips, tcc-bt-device";
		bt_reset-gpio = <&gpb 20 0>;
		status = "okay";
	};
	/* DAUDIO END */

	/* DAUDIO BEGIN */	
	/* Micom */
	serial@76390000 {
		pinctrl-names = "default";
		pinctrl-0 = <&uart12_data>;
		status = "okay";
	};
	/* DAUDIO END */

	/* GPS */
	serial@763a0000 {
		pinctrl-names = "default";
		/* DAUDIO BEGIN */
		/*
		pinctrl-0 = <&uart9_data &uart9_rtscts>;
		*/
		pinctrl-0 = <&uart23_data>;
		/* DAUDIO END */
		status = "okay";
	};

	/* DAUDIO BEGIN */
	/* SIRIUS */
	serial@763b0000 {
		pinctrl-names = "default";
		pinctrl-0 = <&uart20_data>;
		pinctrl-1 = <&sxm_uart20_data>;	
		status = "okay";
                /* check sirius tab */
                int-gpio = <&gpe 7 0>;
	};

	/* EX_CDP */
	serial@763d0000 {
		pinctrl-names = "default";
		pinctrl-0 = <&uart13_data>;
		status = "okay";
	};
	/* DAUDIO END */

	gps-gpio {
		compatible = "telechips, gps-gpio";
		gps_power-gpios = <&gpc 25 0>;
	};

/*
	add dummy key for booting  - start -
 */
	pinctrl@74200000 {
		gpio_powerkey: gpio_powerkey {
			telechips,pins = "gpf-31";
			telechips,pin-function = <0>;
			telechips,input-enable;
			telechips,pull-up;
		};
	};

	gpio_keys_polled {
		compatible = "gpio-keys-polled";
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_powerkey>;
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		autorepeat;
		button@0 {
			label = "Power Key";
			linux,code = <116>;
			gpios = <&gpf 31 1>;
		};
	};
/*
	add dummy key for booting  - end -
 */

	//IPOD, RTC, FM8802
	i2c@76300000 {
		status = "okay";
		port-mux = <22>;
		pinctrl-names = "default";
		pinctrl-0 = <&ws5_ipod_i2c22_bus>;
		
	};

	//touch
	i2c@76310000 {
		status = "okay";
		port-mux = <21>;
		pinctrl-names = "default";
		pinctrl-0 = <&ws5_touch_i2c21_bus>;

		mxt336s@4A {
			compatible = "atmel,mxt336s";
			reg = <0x4A>;
			por-gpios = <&gpf 18 0>;
			int-gpios = <&gpf 21 0>;
			jig-gpios = <&gpg 4 0>;
			scl-gpios = <&gpf 16 0>;
			sda-gpios = <&gpf 15 0>;
			max-x = <799>;
			max-y = <479>;
		};
		melfas_mip4_ts@34{
			compatible = "melfas,mip4_ts";
			reg = <0x34>;
			irq-gpio = <&gpf 21 0>; 
			ce-gpio = <&gpf 18 0>;
		};
		sw17700@28 {				// define new client device(sw17700), slave addr is 0x28
			status = "okay";
			compatible = "siw,sw17700";	// compatible name (see touch_sw17700.c)
			reg = <0x28>;
			interrupt-parent = <&gpf>;
			interrupts = <21 0x02>;
			irqflags = <0x2002>;

			chip_flags = <0x0>;

			reset-gpio = <&gpf 18 1>;
			irq-gpio = <&gpf 21 1>;

			// Caps
			/* Resolution
			 *     [10.25] [12.3]
			 * X :  1920    1920
			 * Y :   720     720
			 */
			max_x = <1920>;
			max_y = <720>;
			max_pressure = <0xff>;
			max_width = <15>;
			max_orientation = <1>;
			max_id = <10>;

			// role
			hw_reset_delay = <210>;
			sw_reset_delay = <90>;
			use_lpwg = <0>;
			use_lpwg_test = <0>;
	
			// firmware
			use_firmware = <1>;		// enable firmware control
			use_fw_upgrade = <1>;		// auto-update during driver initialization
		//	fw_image = "siw/LA103WF5_8_04.img";	// in android -> /lib/firmware/siw/...
		//	fw_image = "{root}/siw/sw17700/LA103WF5_0_01.img";	// in android -> /siw/... (absolute path)
		//	panel_spec = "siw/sw17700/ph1_limit.txt";
		//	panel_spec_mfts = "siw/sw17700/ph1_limit_mfts.txt";
	
			//absoulte path
			prd_in_file = "/sdcard/siw/sw17700_test_spec_V0.1.txt";
			prd_in_file_m = "/sdcard/siw/sw17700_test_spec_mfts_V0.1.txt";
			prd_out_file = "/sdcard/siw/touch_self_test.txt";
		//	prd_out_file_mo_aat = "/sdcard/siw/touch_self_mfts_aat.txt";
		//	prd_out_file_mo_mfo = "/sdcard/siw/touch_self_mfts_mfo.txt";
		//	prd_out_file_mo_mfl = "/sdcard/siw/touch_self_mfts_mfl.txt";
		//	prd_out_file_mo_mcv = "/sdcard/siw/touch_self_mfts_mcv.txt";
		};
	};

    /* i2c SABRE */
    i2c@76320000 { // DAUDIO 76330000 -> 76320000
        status = "okay";
        port-mux = <24>; /* br: port-mux = <20>*/
        pinctrl-names = "default","br";
        pinctrl-0 = <&i2c24_bus>;
	pinctrl-1 = <&i2c20_bus>;
    };

	/* i2c3 */
	i2c@76330000 { // DAUDIO 76320000 -> 76330000
		status = "okay";
		port-mux = <23>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c23_bus>;

       		camera@88 {
			compatible = "tcc_camera,0x88";
			reg = <0x44>; /* check slave address */
		};	

		camera@94 {
			compatible = "tcc_camera,0x94";
			reg = <0x4A>; /* check slave address */
		};
	};

	smui2c@74500000 {
		status = "okay";
	};

	ehci_phy@71EA0010 {
		status = "okay";
		//vbus-ctrl-able;
		//vbus-gpio = <&gpsd 9 0>;
	};

	ehci@71200000 {
		status = "okay";
		/* DAUDIO BEGIN
		vbus-ctrl-able;
		vbus-gpio = <&gpsd 9 0>;
		DAUDIO END */
	};

	ohci@71300000 {
		status = "okay";
		/* DAUDIO BEGIN
		vbus-ctrl-able;
		vbus-gpio = <&gpsd 9 0>;
		DAUDIO END */
	};

	dwc_otg_phy@71EA0100 {
	        status = "okay";
	        vbus-ctrl-able;
	        vbus-gpio = <&gpc 29 0>;
	};

	dwc_otg@71B00000 {
		status = "okay";
		phy-type = <1>;     //pico phy = 1, nano phy = 0
		/* DAUDIO BEGIN
		vbus-ctrl-able;
		vbus-gpio = <&gpsd 8 0>;
		DAUDIO END */
		vbus-ctrl-able;
		vbus-gpio = <&gpc 29 0>;
	};

	/* sd0 WIFI */
	sdhc@76020000 {
		status = "okay";
		controller-id = <0>;
		bus-width = <4>;
		/*max-frequency = <50000000>;*/
		max-frequency = <50000000>;
		cd-inverted;
		/*non-removable;*/
		/* DAUDIO BEGIN */
		/*
		cd-gpios = <&gpf 21 0>;
		//broken-cd;
		//wp-inverted;
		wp-gpios = <&gpf 20 0>;
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd5_clk_idle &sd5_cmd_idle &sd5_bus4_idle>;
		pinctrl-1 = <&sd5_clk &sd5_cmd &sd5_bus4>;
		//vctrl-gpio = <&gpsd 10 0>;
		pwr-gpio = <&gpb 28 0>;

		cap-sdio-irq;
		//cd-polling;
		*/
		pinctrl-names = "default", "active";
		/* pinctrl change */
		pinctrl-0 = <&sd5_clk_idle &sd5_cmd_idle &sd5_bus4_idle>;
		pinctrl-1 = <&sd5_clk &sd5_cmd &sd5_bus4>;

		cap-sdio-irq;
		//cd-polling;
		wifi-polling;
		ext32-enable;
		cap-adma;
		/* DAUDIO END */

		cap2-no-sleep-cmd;
		cap-bus-width-test;

		/* SD 3.0 SDR50 */
		//cap-uhs-sdr50;
		//cap-uhs-driver_type_c;

		/* High-speed DDR (emmc & SD3.0 DDR50) */
		cap-uhs-ddr50;
		cap-1-8V-ddr;
		/* SD 3.0 Command */
		cap-vdd-165-195;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;

		/*keep-power-in-suspend;*/
		/*enable-sdio-wakeup;*/
		/*cap-power-off-card;*/

		/* tcc-clock-control; */
	};

	//eMMC Boot
	sdhc@76020400 {
		status = "okay";
		controller-id = <2>;
		/* DAUDIO BEGIN */
		/*
		bus-width = <4>;
		max-frequency = <50000000>;
		cd-inverted;
		*/
		bus-width = <8>;
		max-frequency = <50000000>;
		non-removable;
		/* DAUDIO END */
		/*cd-gpios = <&gpb 13 0>;*/
		/*wp-gpios = <&gpext1 3 0>;*/
		/* DAUDIO BEGIN */
		/*
		pinctrl-names = "default", "active";
		pinctrl-0 = <&sd0_clk_idle &sd0_cmd_idle &sd0_bus4_idle &out0_clk>;
		pinctrl-1 = <&sd0_clk &sd0_cmd &sd0_bus4 &out0_clk>;
		*/
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus8>;
		/* DAUDIO END */
		clocks = <&clk_peri PERI_SDMMC2 &clk_io IOBUS_SDMMC2 &clk_peri PERI_OUT0>;
		/*pwr-gpio = <&gpext2 10 0>;*/

		cap-sdio-irq;

		/* High-speed SDR */
		cap-sd-highspeed;
		cap-mmc-highspeed;

		/* DAUDIO BEGIN */
		/* set double data rate mode */
		cap-uhs-ddr50;
		cap-1-8V-ddr;
		/* DAUDIO END */

		cap2-no-sleep-cmd;
		cap-bus-width-test;

		/* DAUDIO BEGIN */
		/*
		wifi-polling; // tcm3800 polling
		ext32-enable; // make_rtc
		cap-adma;
		*/
		/* DAUDIO END */

		tcc-kpanic;
		tcc-clock-control;
	};

	//SD Card (map/booting)
	sdhc@76020600 {
		status = "disabled";
		controller-id = <3>;
		/* DAUDIO BEGIN */
		/*
		bus-width = <8>;
		max-frequency = <50000000>;
		non-removable;
		*/
		bus-width = <4>;
		max-frequency = <24000000>;
		cd-inverted;
		cd-gpios = <&gpa 15 0>;
		wp-gpios = <&gpa 13 0>;
		pwr-gpio = <&gpa 14 0>;
		/* DAUDIO END */
		pinctrl-names = "default", "active";
		/* DAUDIO BEGIN */
		/*
		pinctrl-0 = <&sd3_clk_idle &sd3_cmd_idle &sd3_bus8_idle>;
		pinctrl-1 = <&sd3_clk &sd3_cmd &sd3_bus8>;
		*/
		pinctrl-0 = <&sd3_clk_idle &sd3_cmd_idle &sd3_bus4_idle>;
		pinctrl-1 = <&sd3_clk &sd3_cmd &sd3_bus4>;
		/* DAUDIO END */

		cap-sdio-irq;

		cap-bus-width-test;
		cap2-no-sleep-cmd;

		cap-sd-highspeed;
		cap-mmc-highspeed;

		/* DAUDIO BEGIN */
		/*
		cap-1-8V-ddr;
		cap-uhs-ddr50;
		*/
		/* DAUDIO END */

		//tcc-kpanic;

		/* tcc-clock-control; */
	};

	tcm3800 {
		compatible = "telechips, tcm3800";
		wlreg_on-gpio = <&gpf 30 0>; /* SDWF_RST# */
	};

	bcm89335 {
		compatible = "mobis-wifi, bcm89335";
		wlreg_on-gpio = <&gpb 14 0>; /* D-Audio 1.1 MOBIS_WIFI Power On/OFF GPIO */
	};

